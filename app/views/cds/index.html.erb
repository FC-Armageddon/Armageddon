<h1>Cds#index</h1>
<p>Find me in app/views/cds/index.html.erb</p>


<h1>販売商品一覧</h1>

<% @cds.each do |cd| %>
	<p><%= attachment_image_tag cd, :jacket_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image_cd.jpg" %>
	</p>
	<p><%= cd.cd_name %></p>
	<p><%= cd.price %>円</p>
    <p><%= link_to "詳細ページへ", cd_path(cd.id) %></p>
<% end %>
